{
    "name": "addgod/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": ["laravel", "translations", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jesper Kold-Hansen",
            "email": "jkoldhansen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~6",
        "illuminate/translation": "~6",
        "symfony/finder": "~2.3|~3.0",
        "riimu/kit-phpencoder": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Addgod\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addgod\\TranslationManager\\ManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
